<% if not flash[:alert].blank? %>
    <div class="alert alert-error"><button type="button" class="close" data-dismiss="alert">&times;</button><%= flash[:alert] %></div>
<% end %>
<% if not flash[:notice].blank? %>
    <div class="alert alert-info"><button type="button" class="close" data-dismiss="alert">&times;</button><%= flash[:notice] %></div>
<% end %>
